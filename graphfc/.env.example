# GraphFC Environment Variables
# Copy this file to .env and fill in your actual API keys

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL_NAME=gemini-2.0-flash-lite

# OpenAI API Configuration (optional)
OPENAI_API_KEY=your_openai_api_key_here

# HuggingFace Configuration (optional)
HUGGINGFACE_API_TOKEN=your_hf_token_here

# Anthropic API Configuration (optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Model configurations
DEFAULT_MODEL=gpt-3.5-turbo-0125
TEMPERATURE=0.0
MAX_TOKENS=2048

# Dataset paths
HOVER_DATA_PATH=data/hover_dev.json
FEVEROUS_DATA_PATH=data/feverous_dev.jsonl
SCIFACT_DATA_PATH=data/scifact/

# Retrieval settings
TOP_K_RETRIEVAL=5
BM25_INDEX_PATH=data/bm25_index/

# Evaluation settings
BATCH_SIZE=32
MAX_WORKERS=4

# Logging
LOG_LEVEL=INFO
WANDB_PROJECT=graphfc-experiments